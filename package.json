{
    "name": "react-use-observe-changes",
    "version": "13.0.2",
    "description": "The useObserveChanges is a custom React hook that observes changes in specific fields and updates the observed state. It is useful for tracking and managing the state of multiple fields in a React component efficiently.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint 'src/**/*.{ts,tsx}'",
        "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Malnati/useObserveChanges.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "keywords": [
        "React",
        "TypeScript",
        "Node",
        "hooks",
        "hook"
    ],
    "author": "Ricardo Malnati",
    "license": "MIT",
    "dependencies": {
        "loglevel": "^1.9.2",
        "react": "^18.3.0",
        "react-dom": "^18.3.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.10.7",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^4.6.2",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^24.1.1",
        "prettier": "^3.3.3",
        "react-router-dom": "^7.0.2",
        "ts-jest": "^29.2.5",
        "typescript": "^5.1.6"
    }
}
